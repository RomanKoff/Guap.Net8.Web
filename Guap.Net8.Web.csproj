<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>

		<AddRazorSupportForMvc>true</AddRazorSupportForMvc>

		<Version>8.0.0</Version>
		<VersionPrefix>$(Version)</VersionPrefix>
		<Description></Description>
		<Authors>Roman Koff</Authors>
		<Copyright>$(Authors)</Copyright>
		<NeutralLanguage>ru</NeutralLanguage>
	</PropertyGroup>

	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Ans.Net8.Common\Ans.Net8.Common.csproj" />
		<ProjectReference Include="..\Ans.Net8.Web\Ans.Net8.Web.csproj" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Authentication.OpenIdConnect" Version="[8.0.21,9)" />
		<PackageReference Include="NLog" Version="6.0.5" />
		<PackageReference Include="NLog.Web.AspNetCore" Version="6.0.5" />
	</ItemGroup>

	<ItemGroup>
		<Content Remove="_sample_appsettings.json" />
		<None Include="_sample_appsettings.json" />
	</ItemGroup>

</Project>
